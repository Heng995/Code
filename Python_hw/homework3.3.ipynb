    "# BMI 指數判斷"
   ],
   "metadata": {}
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "source": [
    "h = float(input('Please enter your height(cm): '))\r\n",
    "w = float(input('Please enter your weight(kg): '))"
   ],
   "outputs": [],
   "metadata": {}
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "source": [
    "bmi = w / ((h / 100)**2)"
   ],
   "outputs": [],
   "metadata": {}
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "source": [
    "if bmi <= 18:\r\n",
    "    print('You BMI is (bmi:.lf)')"
   ],
